{
  "expo": {
    "name": "quotify",
    "slug": "quotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-screen-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.epoch.quotify",
      "icon": "./assets/icon.png",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-XXXXXXXXXX~YYYYYYYYYY"
      }
    },
    "android": {
      "package": "com.epoch.quotify",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-2290547364208155~4370478475",
          "androidAppId": "ca-app-pub-2290547364208155~4370478475",
          "user_tracking_description": "This identifier will be used to deliver personalized ads to you.",
          "delay_app_measurement_init": true
        }
      ]
    ],
    "scheme": "quotify",
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fc00177d-1e80-4186-ad0c-6fcfe15024ba"
      }
    },
    "owner": "meraki-apps"
  },
  "react-native-google-mobile-ads": {
    "androidAppId": "ca-app-pub-2290547364208155~4370478475"
  }
}

